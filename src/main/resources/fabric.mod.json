{
  "schemaVersion": 1,
  "id": "enchantablespike",
  "version": "${version}",
  "name": "Enchantable Spike",
  "description": "Make Kibe Utilities diamond spikes enchantable.",
  "authors": [
    "BRForgers",
    "armelin1"
  ],
  "contributors": [
    "Abbie"
  ],
  "contact": {
    "homepage": "https://brforgers.com.br/",
    "sources": "https://github.com/BRForgers/EnchantableSpike"
  },
  "license": "MPL-2.0",
  "icon": "assets/enchantablespike/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "br.com.brforgers.mods.enchantablespike.Enchantablespike"
      }
    ],
    "client": [
      {
        "adapter": "kotlin",
        "value": "br.com.brforgers.mods.enchantablespike.EnchantablespikeClient"
      }
    ]
  },
  "mixins": [
    "enchantablespike.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric-api": "*",
    "fabric-language-kotlin": ">=${fabric_kotlin_version}+kotlin.${kotlin_version}",
    "minecraft": "${minecraft_version}",
    "kibe": "*"
  }
}
